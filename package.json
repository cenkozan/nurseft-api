{
  "name": "drommcare-api",
  "version": "0.0.1",
  "author": "Cenk Ozan Kahraman",
  "description": "Backend API for DrommCare web - Healthcare Professionals",
  "engines": {
    "node": ">=50.9"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build -aot -prod && tsc -p server && node dist/server/app.js\"",
    "testbe": "tsc -p server && node_modules/.bin/mocha dist/server/test"
  },
  "private": true,
  "dependencies": {
    "@google-cloud/storage": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "core-js": "^2.4.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.1",
    "jsonwebtoken": "^8.0.1",
    "moment": "^2.18.1",
    "mongoose": "^5.0.6",
    "morgan": "^1.9.0",
    "rxjs": "^5.4.2",
    "tether": "1.4.0"
  },
  "devDependencies": {
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "@types/rx": "^4.1.1",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "concurrently": "^3.3.0",
    "mocha": "^3.4.2",
    "nodemon": "1.11.0",
    "protractor": "~5.1.2",
    "tslint": "^5.9.1",
    "ts-node": "~3.2.0",
    "typescript": "^2.7.2"
  }
}
